{"ast":null,"code":"import createEditForm from \"@/components/forms/create-edit-form.vue\";\nimport buttonValidation from \"@/components/interraction/buttons/validation-button.vue\";\nimport tableExtension from \"@/components/interraction/tables/table-withextension.vue\";\nexport default {\n  name: 'create-server',\n  data() {\n    return {\n      server: {\n        name: null,\n        ip: null,\n        type: null,\n        containers: null\n      }\n    };\n  },\n  components: {\n    createEditForm,\n    buttonValidation,\n    tableExtension\n  }\n};","map":{"version":3,"names":["createEditForm","buttonValidation","tableExtension","name","data","server","ip","type","containers","components"],"sources":["/Users/abdelmadjidbelhanafi/Documents/vsc project/Natixis/new-banxy-rest-api/frontend/src/views/servers/create.vue"],"sourcesContent":["<template>\n    <createEditForm>\n\n        <template #Header>\n            Creating a new server\n        </template>\n\n        <template #formContent>\n            <div class=\"input-name input-holder\">\n                <label>Name</label>\n                <input type=\"text\" placeholder=\"Incident name...\" v-model=\"server.name\">\n            </div>\n            <div class=\"input-ip input-holder\">\n                <label>IP address</label>\n                <input type=\"text\" placeholder=\"Incident name...\" v-model=\"server.ip\">\n            </div>\n            <div class=\"input-type input-holder\">\n                <label>Type</label>\n                <input type=\"text\" placeholder=\"Incident name...\" v-model=\"server.type\">\n            </div>\n            <div class=\"containers input-holder\">\n                <div class=\"forbidden\"></div>\n                <div class=\"add-option\">\n                    <label>Containers</label>\n                    <label style=\"cursor: pointer;\">Add+</label>\n                </div>\n                <div class=\"table-containers\">\n                    <tableExtension >\