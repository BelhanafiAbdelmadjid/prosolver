{"ast":null,"code":"import apiFetch from \"@/common/api.service.js\";\nimport buttonValidation from \"@/components/interraction/buttons/validation-button.vue\";\nimport queryComponentVue from \"@/components/interraction/query/query-navbar.vue\";\nimport usersRoleRect from '@/components/users/role-select.vue';\nimport popUpAdd from \"@/components/interraction/popups/popup-display.vue\";\nimport tableSkeleton from \"@/components/loadingAnimations/table-skeleton.vue\";\nexport default {\n  components: {\n    queryComponentVue,\n    usersRoleRect,\n    buttonValidation,\n    popUpAdd,\n    tableSkeleton\n  },\n  data() {\n    return {\n      users: [],\n      viewPupUp: false,\n      newUser: {},\n      newUserType: \"\",\n      viewPupUpForUpdate: false,\n      loadingusers: true\n    };\n  },\n  watch: {},\n  methods: {\n    async createUser() {\n      if (this.newUserType == \"\" || this.newUser.username == null || this.newUser.username == \"\") {\n        this.emitter.emit(\"sidebar-error\", {\n          error: true,\n          description: \"Please fill the informations correctly.\"\n        });\n        return;\n      }\n      switch (this.newUserType) {\n        case \"Admin\":\n          this.newUser.admin = true;\n          this.newUser.execute = true;\n          break;\n        case \"Worker\":\n          this.newUser.admin = false;\n          this.newUser.execute = true;\n          break;\n        default:\n          this.newUser.admin = false;\n          this.newUser.execute = false;\n          break;\n      }\n      let endpoint = \"/user/create\";\n      apiFetch.post(endpoint, {\n        ...this.newUser\n      }, true, false).then(() => {\n        this.$router.go();\n      });\n    },\n    async deleteUser(user) {\n      let endpoint = '/user/delete';\n      apiFetch.delete(endpoint, {\n        id: user.id\n      }, true, false).then(() => {\n        user.active = false;\n      });\n    },\n    async reactivateUser(user) {\n      let endpoint = \"/user/activate\";\n      apiFetch.update(endpoint, {\n        id: user.id\n      }, true, false).then(() => {\n        user.active = true;\n      });\n    },\n    openPopUpForEdit(user) {\n      this.newUser.admin = user.admin;\n      this.newUser.id = user.id;\n      this.newUser.execute = user.execute;\n      this.newUser.active = user.active;\n      this.newUser.username = user.username;\n      this.viewPupUpForUpdate = true;\n      if (this.newUser.admin) {\n        this.newUserType = 'Admin';\n      } else if (this.newUser.execute) {\n        this.newUserType = 'Worker';\n      } else {\n        this.newUserType = 'Viewer';\n      }\n    },\n    async update() {\n      if (this.newUserType == \"\" || this.newUser.username == null || this.newUser.username == \"\") {\n        this.emitter.emit(\"sidebar-error\", {\n          error: true,\n          description: \"Please fill the informations correctly.\"\n        });\n        return;\n      }\n      switch (this.newUserType) {\n        case \"Admin\":\n          this.newUser.admin = true;\n          this.newUser.execute = true;\n          break;\n        case \"Worker\":\n          this.newUser.admin = false;\n          this.newUser.execute = true;\n          break;\n        default:\n          this.newUser.admin = false;\n          this.newUser.execute = false;\n          break;\n      }\n      let endpoint = \"/user/update\";\n      apiFetch.update(endpoint, {\n        ...this.newUser\n      }, true, false).then(async () => {\n        await this.getUsers();\n        this.newUser = {};\n        this.newUserType = \"\";\n        this.viewPupUpForUpdate = false;\n      });\n    },\n    getUsers(query_user) {\n      this.loadingusers = true;\n      let endpoint = \"/users\";\n      if (query_user != null) {\n        endpoint = endpoint + \"?user=\" + query_user;\n      }\n      apiFetch.get(endpoint, false).then(users => {\n        this.users = users;\n        if (query_user != null && users.length == 0) {\n          this.emitter.emit(\"sidebar-error\", {\n            error: true,\n            description: \"No user matching \" + query_user + \" was found.\"\n          });\n        }\n      });\n    },\n    onInputChanged(input) {\n      this.getUsers(input);\n    }\n  },\n  async beforeMount() {\n    this.getUsers();\n    console.log(\"get users\");\n  }\n};","map":{"version":3,"names":["apiFetch","buttonValidation","queryComponentVue","usersRoleRect","popUpAdd","tableSkeleton","components","data","users","viewPupUp","newUser","newUserType","viewPupUpForUpdate","loadingusers","watch","methods","createUser","username","emitter","emit","error","description","admin","execute","endpoint","post","then","$router","go","deleteUser","user","delete","id","active","reactivateUser","update","openPopUpForEdit","getUsers","query_user","get","length","onInputChanged","input","beforeMount","console","log"],"sources":["/Users/abdelmadjidbelhanafi/Documents/vsc project/Natixis/new-banxy-rest-api/frontend/src/views/users-inetrraction.vue"],"sourcesContent":["<template>\n    <div class=\"holders users-holder\">\n        <queryComponentVue @input-changed=\"onInputChanged\" :theme=\"'flashy'\">\n            <template #Title>\n                Users\n            </template>\n            <template #options>\n                <p @click=\"viewPupUp = true\">create+</p>\n            </template>\n        </queryComponentVue>\n\n        <div v-if=\"loadingusers == false\" class=\"table\">\n            <table>\n                <tr>\n                    <td>Username</td>\n                    <td>User Role</td>\n                    <td>Actions</td>\n                </tr>\n                <tr v-for=\"user in users\" :key=\"user.id\">\n                    <td class=\"username\">{{ user.username }}</td>\n                    <td>\n                        <div class=\"user-roles\">\n                            <usersRoleRect v-if=\"user.admin\" :color=\"'admin'\">\n                                <template #Type>Admin</template>\n                            </usersRoleRect>\n\n                            <usersRoleRect v-else-if=\"user.execute\" :color=\"'worker'\">\n                                <template #Type>Worker</template>\n                            </usersRoleRect>\n\n                            <usersRoleRect v-else-if=\"user.active\" :color=\"'viewer'\">\n                                <template #Type>Viewer</template>\n                            </usersRoleRect>\n\n                            <usersRoleRect v-else :color=\"'deactivated'\">\n                                <template #Type>Deactivated</template>\n                            </usersRoleRect>\n\n                        </div>\n                    </td>\n                    <td>\n                        <div v-if=\"user.active\" class=\"user-actions\">\n                            <div @click=\"openPopUpForEdit(user)\" class=\"user-action\">\n                                <svg width=\"18\" height=\"18\" viewBox=\"0 0 24 24\" fill=\"none\"\n                                    xmlns=\"http://www.w3.org/2000/svg\">\n                                    <path opacity=\"0.5\" fill-rule=\"evenodd\" clip-rule=\"evenodd\"\n                                        d=\"M14.279 2.152C13.909 2 13.439 2 12.5 2C11.561 2 11.092 2 10.721 2.152C10.2285 2.35421 9.83639 2.74377 9.63101 3.235C9.53701 3.458 9.50101 3.719 9.48601 4.098C9.47934 4.3726 9.40274 4.64097 9.26346 4.87772C9.12417 5.11447 8.9268 5.31178 8.69001 5.451C8.44875 5.5851 8.17755 5.65615 7.90154 5.65754C7.62552 5.65894 7.35361 5.59065 7.11101 5.459C6.77301 5.281 6.52801 5.183 6.28601 5.151C5.75656 5.08148 5.22108 5.2238 4.79601 5.547C4.47801 5.789 4.24301 6.193 3.77401 7C3.30401 7.807 3.07001 8.21 3.01701 8.605C2.94701 9.131 3.09101 9.663 3.41701 10.084C3.56501 10.276 3.77401 10.437 4.09701 10.639C4.57401 10.936 4.88001 11.442 4.88001 12C4.88001 12.558 4.57401 13.064 4.09801 13.36C3.77401 13.563 3.56501 13.724 3.41601 13.916C3.25509 14.1239 3.13698 14.361